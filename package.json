{
  "name": "shopify-starter",
  "version": "1.0.0",
  "private": true,
  "author": "Scoutside",
  "description": "A barebones, starting point for developing Shopify themes.",
  "keywords": [
    "shopify",
    "theme"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/scoutside/shopify-starter"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-transform-destructuring": "^7.2.0",
    "@babel/plugin-transform-object-assign": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "autoprefixer": "^9.6.4",
    "babel-loader": "^8.0.5",
    "babel-plugin-dynamic-import-node": "^2.2.0",
    "browser-sync": "^2.26.3",
    "concurrently": "^4.1.0",
    "copyfiles": "^2.1.0",
    "css-loader": "^2.1.0",
    "cssnano": "^4.1.8",
    "file-loader": "^4.2.0",
    "glob": "^7.1.4",
    "js-yaml": "^3.13.1",
    "lost": "^8.3.1",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.14.1",
    "node-watch": "^0.6.0",
    "postcss-clean": "^1.1.0",
    "postcss-flexbox": "^1.0.3",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "postcss-will-change": "^3.0.0",
    "rimraf": "^2.6.3",
    "sass-loader": "^8.0.0",
    "sass-resources-loader": "^2.0.1",
    "webpack": "^4.28.3",
    "webpack-cli": "^3.2.1"
  },
  "scripts": {
    "clean": "rimraf dist/**",
    "setup": "yarn clean && webpack && node fileSetup.js",
    "watch:dev": "concurrently --kill-others \"webpack --watch\" \"node fileWatch.js\" \"theme watch --env=development -d dist --notify=browserUpdate.js\" \"node browserSync.js\"",
    "start:dev": "yarn setup && theme deploy --env=development -d dist && yarn watch:dev",
    "deploy:dev": "theme deploy --env=development -d dist",
    "watch:prod": "concurrently --kill-others \"webpack --watch\" \"node fileWatch.js\" \"theme watch --env=production -d dist --notify=browserUpdate.js\" \"node browserSync.js\"",
    "start:prod": "yarn setup && theme deploy --env=production -d dist && yarn watch:prod",
    "deploy:prod": "theme deploy --env=production -d dist",
    "watch:stag": "concurrently --kill-others \"webpack --watch\" \"node fileWatch.js\" \"theme watch --env=staging -d dist --notify=browserUpdate.js\" \"node browserSync.js\"",
    "start:stag": "yarn setup && theme deploy --env=staging -d dist && yarn watch:dev",
    "deploy:stag": "theme deploy --env=staging -d dist",
    "zip": "zip -r theme.zip dist"
  },
  "dependencies": {
    "dayjs": "^1.8.34",
    "https": "^1.0.0",
    "lazysizes": "^5.2.2",
    "lozad": "^1.14.0",
    "pikaday": "^1.8.0",
    "set-interval-async": "^1.0.32",
    "smoothscroll-polyfill": "^0.4.4",
    "tiny-slider": "^2.9.2",
    "universal-cookie": "^4.0.3"
  }
}
